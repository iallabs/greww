{
  "greww_version" : "0.0.2",
  "babtu_dependencies" : ["skmvs",
                          "cfg-parser"],
  "configuration" : {
    "using" : "machine_config",
    "usage" : ["greww_config",
               "machine_config",
               "manual_config"],
  },
  "configurations" : {
    "greww_config" : {
      "type" : "__ini__",
      "path" : "<GREWW_PATH;paths><'/pkg/config/greww_config.ini'"
    },
    "machine_config" : {
      "type" : "__ini__",
      "path" : "<MACHINE_CONFIG;paths>"
    },
    "manual_config" : {
      "type" : "interactive",
      "cache_file" : "<GREWW_CACHE;paths>/user_config"
    }
  }
}
